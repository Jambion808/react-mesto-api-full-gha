{
  "extends": "airbnb-base"
  ,
    "env": {
        "es2021": true
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "no-console": "off",
      // "linebreak-style": ["error", "windows"],
      "eol-last": ["error", "never"]
    }
}
